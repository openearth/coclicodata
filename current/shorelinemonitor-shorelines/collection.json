  "item_assets": {
    "data": {
      "title": "ShorelineMonitor: Satellite-Derived Shorelines",
      "description": "Parquet dataset with Satellite-Derived Shorelines (SDS) spanning 1984-2024.",
      "roles": [
        "data"
      ],
      "type": "application/vnd.apache.parquet",
      "table:storage_options": {
        "account_name": "coclico"
      },
      "table:columns": [
        {
          "name": "shoreline_id",
          "type": "string",
          "description": "Unique identifier for the shoreline segment."
        },
        {
          "name": "datetime",
          "type": "datetime64[ns]",
          "description": "Timestamp of the shoreline observation."
        },
        {
          "name": "geometry",
          "type": "geometry",
          "description": "Shoreline geometry represented as a geospatial LINESTRING object."
        },
        {
          "name": "bbox",
          "type": "object",
          "description": "Bounding box of the shoreline segment as a dictionary of xmin, ymin, xmax, ymax."
        },
        {
          "name": "quadkey",
          "type": "string",
          "description": "Tile key for spatial indexing using the Bing Maps Tile System."
        },
        {
          "name": "otsu_threshold",
